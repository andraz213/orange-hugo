{
    "uri": "{{ .Permalink }}",
	"title": "{{ .Title}}",
	"content": {{ .Plain | jsonify }},
	"images": [{{ range $tindex, $image := .Params.images }}{{ if $tindex }}, {{ end }}"{{ $image| htmlEscape }}"{{ end }}],
	"download": "{{ .Params.download}}",
	"blog" : "{{ .Params.blog}}",
	"video" : "{{ .Params.video}}",
	"workflows": [{{ range $tindex, $tag := .Params.workflows }}{{ if $tindex }}, {{ end }}"{{ $tag| htmlEscape }}"{{ end }}]
}
